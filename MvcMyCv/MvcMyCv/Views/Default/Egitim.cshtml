@using MvcMyCv.Models.Entity
@model List<tbl_egitim>

<div class="resume-section-content">
    <h2 class="mb-5">Eğitim Hayatım</h2>
    @foreach (var x in Model)
    {
        <div class="d-flex flex-column flex-md-row justify-content-between mb-5">
            <div class="flex-grow-1">
                <h3 class="mb-0">@x.baslik</h3>
                <div class="subheading mb-3">@x.alt_baslik</div>
                <div>@x.alt_baslik_2</div>
                <p>GNO: @x.gnortalama</p>
            </div>
            <div class="flex-shrink-0"><span class="text-primary">@x.tarih</span></div>
        </div>
    }
</div>
