@using MvcMyCv.Models.Entity
@model List<tbl_deneyim>


@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<h2>Deneyim Listesi</h2>
<table class="table table-bordered">
    <tr>
        <th>ID</th>
        <th>Başlık</th>
        <th>Alt Başlık</th>
        <th>Tarih</th>
        <th>Düzenle</th>
        <th>Sil</th>
    </tr>
    @foreach (var item in Model)
    {
    <tr>
        <td>@item.deneyim_id_PK</td>
        <td>@item.baslik</td>
        <td>@item.alt_baslik</td>
        <td>@item.tarih</td>
        <td><a href="/Deneyim/DeneyimGetir/@item.deneyim_id_PK" class="btn btn-warning">Düzenle</a></td>
        <td><a href="/Deneyim/DeneyimSil/@item.deneyim_id_PK" class="btn btn-danger">Sil</a></td>
    </tr>
    }
</table>
<a href="/Deneyim/DeneyimEkle/"class="btn btn-primary" target="_blank">Yeni Deneyim Ekle</a>

