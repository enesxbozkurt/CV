@model MvcMyCv.Models.Entity.tbl_egitim

    @{
        ViewBag.Title = "EgitimEkle";
        Layout = "~/Views/Shared/_AdminLayout.cshtml";
    }
    <br />
    <h4>Yeni Eğitim Bilgisi Ekle</h4>
    <br />
    <form method="post">
        @Html.Label("Başlık")
        @Html.TextBoxFor(x => x.baslik, new { @class = "form-control" })
        @Html.ValidationMessageFor(x => x.baslik, "", new { @style = "background-color:red;" })
        <br />
        @Html.Label("Alt Başlık")
        @Html.TextBoxFor(x => x.alt_baslik, new { @class = "form-control" })
        @Html.ValidationMessageFor(x => x.baslik, "", new { @style = "background-color:red;" })
        <br />
        @Html.Label("Alt Başlık 2")
        @Html.TextBoxFor(x => x.alt_baslik_2, new { @class = "form-control" })
        @Html.ValidationMessageFor(x => x.baslik, "", new { @style = "background-color:red;" })
        <br />
        @Html.Label("GNO")
        @Html.TextBoxFor(x => x.gnortalama, new { @class = "form-control" })
        @Html.ValidationMessageFor(x => x.baslik, "", new { @style = "background-color:red;" })
        <br />
        @Html.Label("Tarih")
        @Html.TextBoxFor(x => x.tarih, new { @class = "form-control" })
        @Html.ValidationMessageFor(x => x.baslik, "", new { @style = "background-color:red;" })
        <br />
        <button class="btn btn-info">Yeni Eğitim Ekle</button>
    </form>

